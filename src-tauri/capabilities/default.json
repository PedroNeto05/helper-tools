{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "../bin/video-downloader/validate-video-url",
          "sidecar": true,
          "args": [
            {
              "validator": "\\S+"
            }
          ]
        },
        {
          "name": "../bin/video-downloader/get-video-info",
          "sidecar": true,
          "args": [
            {
              "validator": "\\S+"
            }
          ]
        }
      ]
    },
    "shell:allow-open",
    "shell:default"
  ]
}